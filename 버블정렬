const isSubsetOf = function (base, sample) {
  base.sort();
  sample.sort();
  let result;
  for(let i = 0; i < base.length; i++){
    for(let j = 0; j < base.length; j++){
      if(base[i] === sample[j]){
          result = sample.shift()
      }
    }
  } if ( sample.length === 0 ) return true
    return false
};
